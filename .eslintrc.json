{
    "env": {
        "browser": true,
        "react-native/react-native": true,
        "node": true,
        "es6": true,
        "jest/globals": true
    },
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly",
        "assert": true,
        "expect": true,
        "__DEV__": true,
        "renderer": true,
        "shallow": true,
        "sinon": true,
        "mount": true,
        "sandbox": true,
        "moxios": true,
        "mockedStore": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true,
            "modules": true,
            "classes": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "extends": ["@react-native-community"],
    "plugins": [
        "jsx-a11y",
        "react",
        "react-native",
        "jest",
        "import"
    ],
    "rules": {
        "strict": 0,
        "jsx-a11y/tabindex-no-positive": 2,
        "react/jsx-uses-vars": 1,
        "react/no-deprecated": 2,
        "react/no-did-mount-set-state": 2,
        "react/no-did-update-set-state": 2,
        "react/no-direct-mutation-state": 2,
        "react/no-is-mounted": 2,
        "react/no-unknown-property": 2,
        "react/react-in-jsx-scope": 2,
        "react/jsx-indent": 2,
        "react/jsx-indent-props": 2,
        "react/jsx-max-props-per-line": [
            2,
            {
                "maximum": 1
            }
        ],
        "react/jsx-no-duplicate-props": 2,
        "react/jsx-no-undef": 2,
        "react/jsx-uses-react": 2,
        "react/jsx-wrap-multilines": 2,
        "react/jsx-sort-props": [
            2,
            {
                "callbacksLast": false,
                "shorthandFirst": false,
                "shorthandLast": false,
                "ignoreCase": true

            }
        ],
        "jsx-quotes": [2, "prefer-double"],
        "react/sort-prop-types": [
            2,
            {
                "callbacksLast": false,
                "ignoreCase": true,
                "requiredFirst": false
            }
        ],
        "no-var": 2,
        "no-shadow": 2,
        "no-shadow-restricted-names": 2,
        "no-undef": 2,
        "no-unused-vars": [
            2,
            {
                "vars": "local",
                "args": "after-used"
            }
        ],
        "comma-dangle": [
            2,
            "never"
        ],
        "no-cond-assign": [
            2,
            "always"
        ],
        "no-debugger": 1,
        "no-alert": 1,
        "no-constant-condition": 1,
        "no-dupe-keys": 2,
        "no-duplicate-case": 2,
        "no-empty": 2,
        "no-ex-assign": 2,
        "no-extra-boolean-cast": 0,
        "no-extra-semi": 2,
        "no-func-assign": 2,
        "no-inner-declarations": 2,
        "no-invalid-regexp": 2,
        "no-irregular-whitespace": 2,
        "no-obj-calls": 2,
        "no-sparse-arrays": 2,
        "no-unreachable": 2,
        "use-isnan": 2,
        "block-scoped-var": 2,
        "curly": [
            2,
            "multi-line"
        ],
        "default-case": 2,
        "dot-notation": [
            2,
            {
                "allowKeywords": true
            }
        ],
        "eqeqeq": 2,
        "guard-for-in": 2,
        "no-caller": 2,
        "no-eq-null": 2,
        "no-eval": 2,
        "no-extend-native": 2,
        "no-extra-bind": 2,
        "no-fallthrough": 2,
        "no-floating-decimal": 2,
        "no-implied-eval": 2,
        "no-lone-blocks": 2,
        "no-loop-func": 2,
        "no-multi-str": 2,
        "no-native-reassign": 2,
        "no-new": 2,
        "no-new-func": 2,
        "no-new-wrappers": 2,
        "no-octal": 2,
        "no-octal-escape": 2,
        "no-proto": 2,
        "no-redeclare": 2,
        "no-return-assign": 2,
        "no-script-url": 2,
        "no-self-compare": 2,
        "no-sequences": 2,
        "no-throw-literal": 2,
        "no-with": 2,
        "radix": 2,
        "wrap-iife": [
            2,
            "any"
        ],
        "yoda": 2,
        "indent": [
            2,
            4,
            {
                "SwitchCase": 1
            }
        ],
        "brace-style": [
            2,
            "1tbs",
            {
                "allowSingleLine": true
            }
        ],
        "camelcase": [
            2,
            {
                "properties": "never"
            }
        ],
        "comma-spacing": [
            2,
            {
                "before": false,
                "after": true
            }
        ],
        "comma-style": [
            2,
            "last"
        ],
        "eol-last": 2,
        "key-spacing": [
            2,
            {
                "beforeColon": false,
                "afterColon": true
            }
        ],
        "new-cap": [
            2,
            {
                "newIsCap": true
            }
        ],
        "no-multiple-empty-lines": [
            1,
            {
                "max": 2
            }
        ],
        "no-nested-ternary": 2,
        "no-new-object": 2,
        "no-spaced-func": 2,
        "no-trailing-spaces": 2,
        "no-underscore-dangle": 0,
        "one-var": [
            2,
            "never"
        ],
        "padded-blocks": [
            2,
            "never"
        ],
        "semi-spacing": [
            2,
            {
                "before": false,
                "after": true
            }
        ],
        "keyword-spacing": [
            2,
            {
                "before": true,
                "after": true,
                "overrides": {}
            }
        ],
        "spaced-comment": [
            2,
            "always"
        ],
        "no-console": 2,
        "space-before-function-paren": [
            2,
            {
                "named": "never",
                "anonymous": "always"
            }
        ],
        "semi": ["error", "never"],
        "quotes": ["error", "double"],
        "func-names": 0,
        "consistent-this": [
            2,
            "this"
        ],
        "func-style": [
            0,
            "expression"
        ],
        "generator-star-spacing": [
            2,
            {
                "after": false,
                "before": true
            }
        ],
        "max-nested-callbacks": [
            2,
            3
        ],
        "new-parens": 2,
        "no-array-constructor": 2,
        "no-else-return": 0,
        "no-inline-comments": 2,
        "no-lonely-if": 2,
        "no-mixed-spaces-and-tabs": [
            2,
            "smart-tabs"
        ],
        "no-ternary": 0,
        "operator-assignment": 0,
        "operator-linebreak": [
            2,
            "after"
        ],
        "quote-props": [
            2,
            "as-needed"
        ],
        "sort-vars": [
            0,
            {
                "ignoreCase": true
            }
        ],
        "space-in-brackets": [
            0,
            "never",
            {
                "arraysInArrays": false,
                "arraysInObjects": false,
                "singleValue": false,
                "objectsInArrays": false,
                "objectsInObjects": false,
                "propertyName": false
            }
        ],
        "space-in-parens": [
            2,
            "never"
        ],
        "space-infix-ops": 2,
        "space-unary-ops": [
            2,
            {
                "words": true,
                "nonwords": false
            }
        ],
        "jest/no-disabled-tests": "error",
        "jest/no-focused-tests": "error",
        "jest/no-identical-title": "error"
    }
}
